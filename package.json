{
  "name": "keylogger",
  "productName": "Key Logger",
  "version": "1.0.0",
  "description": "This application logs any key that the user press on the keyboard.",
  "main": "main.js",
  "scripts": {
    "app": "set NODE_ENV=dev&& electron .",
    "watch": "nodemon --exec electron .",
    "postinstall": "electron-rebuildi",
    "build": "electron-packager . --icon=keylogger.ico --overwrite --asar --platform=win32 --prune=true"
  },
  "keywords": [
    "keyboard",
    "keypress"
  ],
  "author": "Saeed-Sufi <Sufi.geo@gmail.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-packager": "^15.2.0",
    "nodemon": "^2.0.7"
  }
}
